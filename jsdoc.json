{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": [
      "app/api",
      "app/auth",
      "app/components",
      "app/components/forms",
      "app/config",
      "app/hooks",
      "app/navigation",
      "app/screens",
      "app/utility",
      "App.js"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
